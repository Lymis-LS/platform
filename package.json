{
  "name": "bim",
  "version": "1.0.0",
  "description": "A Vue.js project",
  "author": "",
  "private": true,
  "scripts": {
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "start": "npm run dev",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e",
    "lint": "eslint --ext .js,.vue src test/unit test/e2e/specs",
    "build": "node build/build.js",
    "cypress:open": "cypress open",
    "mocha": "mocha --compilers js:babel-core/register test/unit/specs/**/*.js",
    "export-translations": "node build/sync-translations.js",
    "cypress:run": "cypress run --reporter mocha-multi-reporters --reporter-options configFile=config/test-report.json",
    "cypress:install": "cypress install",
    "cypress:test": "start-server-and-test start http-get://localhost:8080 cypress:run",
    "generate-icons": "vsvg -s ./src/assets/images/icons/compiled -t ./src/icons --es6",
    "optimize-svg": "svgo -f ./src/assets/images/icons -o ./src/assets/images/icons/compiled",
    "import-material-icon": "node build/get-material-icons.js",
    "svg-process": "npm run import-material-icon && npm run optimize-svg && npm run generate-icons"
  },
